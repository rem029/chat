{
	"name": "@chat/backend",
	"description": "Backend for store project",
	"version": "0.0.0-alpha.6",
	"main": "src/index.js",
	"type": "commonjs",
	"private": false,
	"scripts": {
		"start": "yarn clean-logs && ts-node-dev --project tsconfig.json --no-notify --transpile-only -r tsconfig-paths/register src/index.ts",
		"build": "yarn build:clean && tsc -b && resolve-tspaths",
		"build:production": "yarn build:clean && webpack --mode=production --node-env=production",
		"build:staging": "yarn build:clean && webpack --mode=development --node-env=development",
		"build:run": "node ./dist/index.js",
		"build:clean": "rimraf ./dist",
		"dev": "nodemon --exec ts-node-dev -r tsconfig-paths/register ./src/index.ts DEBUG=knex:query",
		"clean-logs": "rimraf ./src/__logs",
		"lint": "eslint src/**/*.ts",
		"migrate-latest": "knex migrate:latest",
		"migrate-rollback": "knex migrate:rollback",
		"db-create": "set PGPASSWORD=123& createdb -U postgres -p 5433 -O postgres store-db",
		"db-import": "set PGPASSWORD=123& psql -U postgres -p 5433 store-db < ./sql/store-db.sql"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/jsonwebtoken": "^8.5.8",
		"@types/knex": "^0.16.1",
		"@types/node-statsd": "^0.1.3",
		"@types/response-time": "^2.3.5",
		"@types/socket.io": "^3.0.2",
		"bcrypt": "^5.0.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"express": "^4.17.1",
		"express-async-errors": "^3.1.1",
		"form-data": "^4.0.0",
		"iconv-lite": "^0.6.3",
		"jsonwebtoken": "^8.5.1",
		"knex": "^1.0.7",
		"mailgun.js": "^8.2.1",
		"moment-timezone": "^0.5.34",
		"node-fetch": "^3.3.1",
		"node-statsd": "^0.1.1",
		"pg": "^8.10.0",
		"redis": "^4.6.5",
		"response-time": "^2.3.2",
		"socket.io": "^4.4.1",
		"tsconfig-paths": "^3.14.0",
		"winston": "^3.7.2"
	},
	"devDependencies": {
		"@babel/core": "^7.21.4",
		"@babel/preset-env": "^7.21.4",
